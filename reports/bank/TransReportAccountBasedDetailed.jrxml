	<queryString>
		<![CDATA[select b.name, b.account_no, t.exc_code, t.trans_date, t.receipt_no, t.trans_no, t.debt, t._right, t.credit, (t.debt - t.credit) as balance, t._right, t.description, (select sum(st.debt - st.credit) from bank_trans st where st.bank_id = t.bank_id and st.trans_date < $P{FIRST_DATE} and st.exc_code = t.exc_code) as transfer
from bank_trans t
inner join bank b on b.id = t.bank_id and b.is_active = $P!{TRU}
left join bank_trans_source ts on ts.id = t.trans_source_id and ts.is_active = $P!{TRU}
$P!{TRANS_POINT_SQL}
$P!{PRIVATE_CODE_SQL}
where 1=1
$P!{QUERY_STRING}
order by b.name, t.exc_code, t.trans_date, ts.name]]>
