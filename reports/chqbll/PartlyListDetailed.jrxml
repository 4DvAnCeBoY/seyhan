		<![CDATA[select t.id, t.is_customer, t.sort, portfolio_no, serial_no, last_contact_name, due_date, last_step, owner, surety, bank_name, payment_place, t.amount, total_paid, t.exc_code, t.description, cb.name as type_name, s.name as sname, pd.trans_date, pd.amount as pdamount, pd.description as ddescription
from chqbll_payroll_detail t
inner join chqbll_detail_partial pd on pd.detail_id = t.id
left join contact c on c.id = t.contact_id and c.is_active = $P!{TRU}
left join safe s on s.id = pd.safe_id and s.is_active = $P!{TRU}
left join chqbll_type cb on cb.id = t.cbtype_id and cb.is_active = $P!{TRU}
left join chqbll_payroll_source ts on ts.id = t.trans_source_id and ts.is_active = $P!{TRU}
$P!{TRANS_POINT_SQL}
$P!{PRIVATE_CODE_SQL}
where 1=1
$P!{QUERY_STRING}
