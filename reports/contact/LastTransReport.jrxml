	<queryString>
		<![CDATA[select c.name, c.phone, c.mobile_phone, t.trans_date, t.exc_code, t._right, t.amount
from contact c
inner join contact_trans t on t.contact_id = c.id
inner join (select contact_id, max(t.id) as trans_id from contact_trans as t $P!{TRANS_POINT_SQL} where 1=1 $P!{QUERY_STRING_PART_2} group by t.contact_id) as lt on lt.trans_id = t.id and lt.contact_id = c.id
where is_active = $P!{TRU}
$P!{QUERY_STRING_PART_1}
ORDER BY c.name]]>
