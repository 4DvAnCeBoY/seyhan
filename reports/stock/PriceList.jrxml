	<queryString>
		<![CDATA[select s.code, s.name, b.barcode, ea1.name as ea1_name, ea2.name as ea2_name, s.exc_code, (s.sell_price $P!{PRICE_MULTIPLIER}) as price from stock as s
left join stock_barcode as b on b.stock_id = s.id and b.is_primary = true
left join stock_extra_fields as ea1 on ea1.id = s.extra_field0_id
left join stock_extra_fields as ea2 on ea2.id = s.extra_field1_id
where s.is_active = $P!{TRU}
$P!{QUERY_STRING}
$P!{CATEGORY_SQL}
$P!{ORDER_BY}]]>
